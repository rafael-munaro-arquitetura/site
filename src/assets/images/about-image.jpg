<svg width="600" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="aboutGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#545943;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#9BA187;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#E8DACB;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="800" fill="url(#aboutGrad)"/>
  
  <!-- Grid Pattern -->
  <g opacity="0.15">
    <line x1="0" y1="200" x2="600" y2="200" stroke="#FFFFFF" stroke-width="1"/>
    <line x1="0" y1="400" x2="600" y2="400" stroke="#FFFFFF" stroke-width="1"/>
    <line x1="0" y1="600" x2="600" y2="600" stroke="#FFFFFF" stroke-width="1"/>
    <line x1="200" y1="0" x2="200" y2="800" stroke="#FFFFFF" stroke-width="1"/>
    <line x1="400" y1="0" x2="400" y2="800" stroke="#FFFFFF" stroke-width="1"/>
  </g>
  
  <!-- Central Design -->
  <g transform="translate(200, 300)">
    <rect x="0" y="0" width="200" height="200" fill="none" stroke="#FFFFFF" stroke-width="4" opacity="0.8"/>
    <path d="M100 50L150 100L100 150L50 100Z" fill="#B66C48" opacity="0.9"/>
    <circle cx="100" cy="100" r="60" fill="none" stroke="#FFFFFF" stroke-width="3" opacity="0.7"/>
  </g>
  
  <!-- Text -->
  <text x="300" y="600" font-family="Inter, sans-serif" font-size="20" font-weight="600" fill="#FFFFFF" text-anchor="middle">
    Rafael Munaro
  </text>
  <text x="300" y="630" font-family="Inter, sans-serif" font-size="16" fill="#E8DACB" text-anchor="middle">
    Arquiteto & Designer
  </text>
</svg>

