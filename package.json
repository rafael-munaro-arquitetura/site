{
  "name": "site-rafael-munaro-arquitetura",
  "version": "2.4.1",
  "description": "Site institucional para Rafael Munaro Arquitetura",
  "main": "src/index.html",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint:check": "eslint src/**/*.js --max-warnings 0",
    "format": "prettier --write src/**/*.{js,css,html,json,md}",
    "format:check": "prettier --check src/**/*.{js,css,html,json,md}",
    "format:staged": "prettier --write $(git diff --cached --name-only --diff-filter=ACM | grep -E '\\.(js|css|html|json|md)$' | tr '\\n' ' ')",
    "clean": "rm -rf dist build .vite",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "serve": "npm run build && npm run preview",
    "analyze": "npx vite-bundle-analyzer dist",
    "size": "npx size-limit",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "prepare": "husky install || true",
    "precommit": "npm run lint:check && npm run format:check",
    "release": "npm run build && npm run preview"
  },
  "keywords": [
    "arquitetura",
    "site-institucional",
    "web",
    "html",
    "css",
    "javascript"
  ],
  "author": "Rafael Munaro Arquitetura",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-legacy": "^7.2.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.6",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.0.0",
    "size-limit": "^8.2.0",
    "vite": "^7.1.12",
    "vite-bundle-analyzer": "^0.8.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
